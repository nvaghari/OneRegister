@model List<OneRegister.Domain.Model.Account.UserListModel>
@{
    ViewData["Title"] = "List";
}
@section Styles{
    <link href="~/lib/dataTables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/dataTables/responsive/css/responsive.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/lib/gijgo/combined/css/gijgo.min.css" rel="stylesheet" />
    <link href="~/lib/ninotree/ninotree.css" rel="stylesheet" />
}
<h4 class="text-primary">User List</h4>

<div class="row mt-3">
    <div class="col-12">
        <div>
            <table id="userListGrid" class="table table-striped table-bordered dt-responsive nowrap w-100">
                <thead class="bg-primary text-light">
                    <tr>
                        <th>Action</th>
                        <th>Id</th>
                        <th>User Name</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>Organization</th>
                        <th>Status</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>

@section Scripts{
    @{ await Html.RenderPartialAsync("_DataTableScripts"); }
    <script type="text/javascript" src="~/lib/gijgo/combined/js/gijgo.min.js" ></script>
    <script type="text/javascript" src="~/lib/ninotree/ninotree.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/js/account/UserList.js" asp-append-version="true"></script>
}