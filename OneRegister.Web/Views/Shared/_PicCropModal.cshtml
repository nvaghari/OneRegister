<div class="modal" id="picModal" tabindex="-1" role="dialog" aria-labelledby="PictureCroppingModal" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Cropping Image</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body pic-modal-body">
                <div id="cropper"></div>
            </div>
            <div class="modal-footer">
                <button id="btn-crop" type="button" class="btn btn-warning">Crop Image</button>
            </div>
        </div>
    </div>
</div>
